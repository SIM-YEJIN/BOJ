#define _CRT_SECURE_NO_WARNINGS
#include <iostream>
#include <cstdio>

int main5() {
	int num;
	scanf("%d", &num);

	for (int i = 0; i < num; i++) {
		for (int j = num-1; j > i; j--) {
			printf(" ");
		}
		for (int k = 0; k <= i; k++) {
			printf("*");
		}
		for (int l = 0; l < i; l++) {
			printf("*");
		}
		printf("\n");
	}
	return 0;
}
