#define _CRT_SECURE_NO_WARNINGS
#include <iostream>
#include <cstdio>
using namespace std;

int main11720() {
	int num; // 숫자 개수
	scanf("%d ", &num); // 공백을 줘서 개행문자 포함시킴

	int sum = 0; // 합
	while (num--) {
		char ch; // 입력 숫자(문자형)
		scanf("%c", &ch);
		sum += ch - '0'; // 문자를 숫자로 변환
	}
	printf("%d\n", sum);
	
	return 0;
}
